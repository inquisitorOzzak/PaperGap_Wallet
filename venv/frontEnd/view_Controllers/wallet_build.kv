<Create_Wallet>
    BoxLayout:
        orientation: "horizontal"
        padding: 15

    Label:
        text: "In order to access your bitcoin wallet we will generate a random mnemonic"
        font_size: 20
        width: root.width
        height: 60


    Spinner:
        id: spinner_id
        text: "Word Number"
        values: ["16", "20", "24"]
        on_text : root.word_Spinner_Clicked(spinner_id.text)
